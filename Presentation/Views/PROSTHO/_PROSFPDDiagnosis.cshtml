                                    @Html.LabelFor(model => model.DiagAxialInclination, new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.TextAreaFor(model => model.DiagAxialInclination, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divDiagWearfacets">
