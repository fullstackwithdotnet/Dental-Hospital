@section scripts
{
    <script>
        $(document).ready(function () {

            $("#FirstDentalVisit").change(function () {

                var dropdowntext = $("#FirstDentalVisitId option:selected").text();
                if (dropdowntext == "Yes") {
                    $("#RegDate").hide();
                    $("#TreatmentDone").hide();
                }
                else {
                    $("#RegDate").show();
                    $("#TreatmentDone").show();

                }
            });

            $("#SleepWithBottle").change(function () {

                var dropdowntext = $("#SleepWithBottleId option:selected").text();
                if (dropdowntext == "No") {
                    $("#BottleContents").hide();
                }
                else {
                    $("#BottleContents").show();
                }
            });

            $("#AllergyHistory").change(function () {

                var dropdowntext = $("#AllergyHistoryId option:selected").text();
                if (dropdowntext == "No") {
                    $("#DrugDesc").hide();
                    $("#FoodDesc").hide();
                    $("#InhalentDesc").hide();
                }
                else {
                    $("#DrugDesc").show();
                    $("#FoodDesc").show();
                    $("#InhalentDesc").show();
                }

            });


            $("#EruptedTooth").change(function () {

                var dropdowntext = $("#EruptedToothId option:selected").text();
                if (dropdowntext == "No") {
                    $("#EruptedToothAtBirthDesc").hide();
                    $("#EruptedtoothT").hide();

                }
                else {
                    $("#EruptedToothAtBirthDesc").show();
                    $("#EruptedtoothT").show();

                }

            });

            $('#UnfavourableDentalExpId').on('change', function (e) {

                var dropdowntext = $("#UnfavourableDentalExpId option:selected").text();
                if (dropdowntext == "Yes")
                    $('#UnfavourableDentalExpDesc').prop('disabled', false);
                else
                    $('#UnfavourableDentalExpDesc').prop('disabled', true);
                $('#UnfavourableDentalExpDesc').val("");
            });


            $('#InformerId').on('change', function (e) {

                var dropdowntext = $("#InformerId option:selected").text();
                if (dropdowntext == "Any Other")
                    $('#AnyOther').prop('disabled', false);
                else
                    $('#AnyOther').prop('disabled', true);
                $('#AnyOther').val("");
            });


            $('#ToothBrushMethodId').on('change', function (e) {

                var dropdowntext = $("#ToothBrushMethodId option:selected").text();
                if (dropdowntext == "Others")
                    $('#OtherBrushingMethod').prop('disabled', false);
                else
                    $('#OtherBrushingMethod').prop('disabled', true);
                $('#OtherBrushingMethod').val("");
            });


            $('#RespiratoryId').on('change', function (e) {

                var dropdowntext = $("#RespiratoryId option:selected").text();
                if (dropdowntext == "Others")
                    $('#RespiratoryOthers').prop('disabled', false);
                else
                    $('#RespiratoryOthers').prop('disabled', true);
                $('#RespiratoryOthers').val("");
            });


            $('#CvsId').on('change', function (e) {

                var dropdowntext = $("#CvsId option:selected").text();
                if (dropdowntext == "Others")
                    $('#CvsOthers').prop('disabled', false);
                else
                    $('#CvsOthers').prop('disabled', true);
                $('#CvsOthers').val("");
            });


            $('#CnsId').on('change', function (e) {

                var dropdowntext = $("#CnsId option:selected").text();
                if (dropdowntext == "Others")
                    $('#CnsOthers').prop('disabled', false);
                else
                    $('#CnsOthers').prop('disabled', true);
                $('#CnsOthers').val("");
            });

            $('#ImmuneSystemId').on('change', function (e) {

                var dropdowntext = $("#ImmuneSystemId option:selected").text();
                if (dropdowntext == "Others")
                    $('#ImmuneSystemOthers').prop('disabled', false);
                else
                    $('#ImmuneSystemOthers').prop('disabled', true);
                $('#ImmuneSystemOthers').val("");
            });


            $('#RenalDiseaseId').on('change', function (e) {

                var dropdowntext = $("#RenalDiseaseId option:selected").text();
                if (dropdowntext == "Others")
                    $('#RenalDiseaseOthers').prop('disabled', false);
                else
                    $('#RenalDiseaseOthers').prop('disabled', true);
                $('#RenalDiseaseOthers').val("");
            });


            $('#MaxillaE').on('change', function () {
                MaxillaCalculation();
            });
            $('#MaxillaG').on('change', function () {
                MaxillaCalculation();
            });
            $('#MaxillaF').on('change', function () {
                MaxillaCalculation();
            });
            $('#MaxillaH').on('change', function () {
                MaxillaCalculation();
            });

            var MaxillaCalculation = function () {
                var $MaxillaE = 0;
                var $MaxillaG = 0;
                var $MaxillaF = 0;
                var $MaxillaH = 0;
                var $MaxillaTotal = 0;

                if ($('#MaxillaE').val() != '') {
                    $MaxillaE = parseFloat($('#MaxillaE').val());
                }
                if ($('#MaxillaG').val() != '') {
                    $MaxillaG = parseFloat($('#MaxillaG').val());
                }
                if ($('#MaxillaF').val() != '') {
                    $MaxillaF = parseFloat($('#MaxillaF').val());
                }
                if ($('#MaxillaH').val() != '') {
                    $MaxillaH = parseFloat($('#MaxillaH').val());
                }

                $MaxillaTotal = $MaxillaE + $MaxillaG + $MaxillaF + $MaxillaH;
                $('#MaxillaTotal').val($MaxillaTotal)
            }

            $('#MandibleA').on('change', function () {
                MandibleCalculation();
            });
            $('#MandibleB').on('change', function () {
                MandibleCalculation();
            });
            $('#MandibleC').on('change', function () {
                MandibleCalculation();
            });
            $('#MandibleD').on('change', function () {
                MandibleCalculation();
            });

            var MandibleCalculation = function () {
                var $MandibleA = 0;
                var $MandibleB = 0;
                var $MandibleC = 0;
                var $MandibleD = 0;
                var $MandibleTotal = 0;

                if ($('#MandibleA').val() != '') {
                    $MandibleA = parseFloat($('#MandibleA').val());
                }
                if ($('#MandibleB').val() != '') {
                    $MandibleB = parseFloat($('#MandibleB').val());
                }
                if ($('#MandibleC').val() != '') {
                    $MandibleC = parseFloat($('#MandibleC').val());
                }
                if ($('#MandibleD').val() != '') {
                    $MandibleD = parseFloat($('#MandibleD').val());
                }

                $MandibleTotal = $MandibleA + $MandibleB + $MandibleC + $MandibleD;
                $('#MandibleTotal').val($MandibleTotal)
            }

            MaxillaCalculation();
            MandibleCalculation();

        });


