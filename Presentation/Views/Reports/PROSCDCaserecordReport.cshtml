                                @Html.Partial("../Reports/_PatientDetailsReportHeader", Model.patientInformationViewModel)
                            </div>
                        </div>
                    </div>

                    <h4><strong>History</strong></h4>
                    <table class="table table-bordered">
                        <tr style='@IsVisible(@Model.ChiefComplaint)'>
                            <td class="col30">@Html.DisplayNameFor(model => model.ChiefComplaintName)</td>
                            <td class="col70">@Html.DisplayFor(model => model.ChiefComplaint)</td>
                        </tr>
                    </table>

                    @if (Model.MedicalHistory != null)
                    {

                        <h5 class="text-center">History</h5>
                        <table class="table table-bordered">
                            <tr style='@IsVisible(@Model.MedicalHistory)'>
                                <td class="col30">@Html.DisplayNameFor(model => model.MedicalHistory)</td>
                                <td class="col70">@Html.DisplayFor(model => model.MedicalHistory)</td>
                            </tr>
