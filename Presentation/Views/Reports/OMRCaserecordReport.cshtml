@functions{
    public string IsVisible(string propertyName)
    {
        if (propertyName == null)
        {
            return "display:none";
        }
        return "";
    }

}

<div class="box box-default box-solid">
    <div class="box-header with-border">
        <h3 class="box-title"><strong>Oral Medicine and Radiology Department Patient Casesheet</strong></h3>
        <div class="box-tools pull-right text-black ">
            Case Record #
            <strong>@Html.DisplayFor(model => model.OMRNo)</strong>
        </div>
    </div>
    <div class="box-body">
