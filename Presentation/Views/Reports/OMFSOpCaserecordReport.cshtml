@functions{
    public string IsVisible(string propertyName)
    {
        if (propertyName == null)
        {
            return "display:none";
        }
        return "";
    }

}

<div class="box box-default box-solid">
    <div class="box-header with-border">
        <h3 class="box-title"><strong>Oral & Maxillofacial Surgery Casesheet</strong></h3>
        <div class="box-tools pull-right text-black ">
            Case Record #
            <strong>@Html.DisplayFor(model => model.OMFSOpNo)</strong>
        </div>
    </div>
    <div class="box-body">
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
                    <h4><strong>Patient Details</strong></h4>
                    @Html.Partial("../Reports/_PatientDetailsReportHeader", Model.patientInformationViewModel)

                    <h4><strong>History</strong></h4>
                    <table class="table table-bordered">
                        <tr style='@IsVisible(@Model.ChiefComplaint)'>
                            <td class="col30">@Html.DisplayNameFor(model => model.ChiefComplaintName)</td>
                            <td class="col70">@Html.DisplayFor(model => model.ChiefComplaint)</td>
                        </tr>
                    </table>

                    @if (Model.HPIModeOfOnset != null || Model.HPIDuration != null || Model.HPIFrequency != null || Model.HPILocation != null || Model.HPISeverity != null || Model.HPIProgression != null || Model.HPIAggrevating != null ||
                            Model.HPIFactors != null || Model.HPIRelievingFact != null || Model.HPIAnyPreviousTreat != null)
                    {
                        <h5 class="text-center">History of Present Illness</h5>
