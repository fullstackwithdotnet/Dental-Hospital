            $Lower3Lt = parseFloat($("#Lower3Lt").val());
            $Lower4Rt = parseFloat($("#Lower4Rt").val());
            $Lower5Rt = parseFloat($("#Lower5Rt").val());
            $Lower6Rt = parseFloat($("#Lower6Rt").val());
            $Lower4Lt = parseFloat($("#Lower4Lt").val());
            $Lower5Lt = parseFloat($("#Lower5Lt").val());
            $Lower6Lt = parseFloat($("#Lower6Lt").val());
            TotUpper6Tm =parseFloat( $("#Upper6Tm").val());
