        $("#Lower6Rt").on("keydown", function () {
            
            Lower6Calculation();
        });
        $("#Lower4Lt").on("keydown", function () {
            
            Lower6Calculation();
        });
        $("#Lower5Lt").on("keydown", function () {
            
            Lower6Calculation();
        });
        $("#Lower6Lt").on("keydown", function () {
            
            Lower6Calculation();
        });




        var Upper6Calculation = function () {
            var $Upper1Rt = 0; var $Upper2Rt = 0; var $Upper3Rt = 0;
            var $Upper1Lt = 0; var $Upper2Lt = 0; var $Upper3Lt = 0;
            var $Upper1Rt = 0; var $Upper2Rt = 0; var $Upper3Rt = 0;
            var $Upper1Lt = 0; var $Upper2Lt = 0; var $Upper3Lt = 0;
            var $Upper4Rt = 0; var $Upper5Rt = 0; var $Upper6Rt = 0;
            var $Upper4Lt = 0; var $Upper5Lt = 0; var $Upper6Lt = 0;
            var TotUpper6Tm = 0; var TotLower6Tm = 0; var TotUpper12 = 0;
            var TotLower12 = 0; var Incisors = 0; var TotIncisor = 0;
            var Anterior = 0; var Overall = 0; var Mandibular6Excess = 0;
            var Maxillary6Excess = 0;
            $Upper1Rt = parseFloat($("#Upper1Rt").val());
            $Upper2Rt = parseFloat($("#Upper2Rt").val());
            $Upper3Rt = parseFloat($("#Upper3Rt").val());
            $Upper1Lt = parseFloat($("#Upper1Lt").val());
            $Upper2Lt = parseFloat($("#Upper2Lt").val());
            $Upper3Lt = parseFloat($("#Upper3Lt").val());
            $Upper4Rt = parseFloat($("#Upper4Rt").val());
            $Upper5Rt = parseFloat($("#Upper5Rt").val());
            $Upper6Rt = parseFloat($("#Upper6Rt").val());
            $Upper4Lt = parseFloat($("#Upper4Lt").val());
            $Upper5Lt = parseFloat($("#Upper5Lt").val());
            $Upper6Lt = parseFloat($("#Upper6Lt").val());
            TotLower6Tm = parseFloat($("#Lower6Tm").val());
            TotLower12 = parseFloat($("#Lower12Tm").val());

            TotUpper6Tm =parseFloat( $Upper1Rt + $Upper2Rt + $Upper3Rt + $Upper1Lt + $Upper2Lt + $Upper3Lt);
            $("#Upper6Tm").val(TotUpper6Tm);

            TotUpper12 = parseFloat(TotUpper6Tm + $Upper4Rt + $Upper5Rt + $Upper6Rt + $Upper4Lt + $Upper5Lt + $Upper6Lt);
            $("#Upper12Tm").val(TotUpper12);

            Anterior = parseFloat(((TotLower6Tm * 100) / TotUpper6Tm).toFixed(2));
            $("#AnteriorRatio").val(Anterior);

            Overall = parseFloat(((TotLower12 * 100) / TotUpper12).toFixed(2));
            $("#OverallRatio").val(Overall);

            TotIncisor = $Upper1Rt + $Upper1Lt + $Upper2Rt + $Upper2Lt;
            $("#SumIncisors").val(TotIncisor);

            //6TM
            Mandibular6Excess = parseFloat((TotLower6Tm - ((TotUpper6Tm * 77.2 ) / 100)).toFixed(2));
            $("#MandibularExcess6Tm").val(Mandibular6Excess);

            Maxillary6Excess = parseFloat((TotUpper6Tm - ((TotLower6Tm * 100) / 77.2 )).toFixed(2));
            $("#MaxillaryExcess6Tm").val(Maxillary6Excess);

            //12TM
            Mandibular12Excess = parseFloat((TotLower12 - ((TotUpper12 * 91.3) / 100)).toFixed(2));
            $("#MandibularExcess12Tm").val(Mandibular12Excess);

            Maxillary12Excess = parseFloat((TotUpper12 - ((TotLower12 * 100) / 91.3)).toFixed(2));
            $("#MaxillaryExcess12Tm").val(Maxillary12Excess);
            
        }

        var Lower6Calculation = function () {
            var $Lower1Rt = 0; var $Lower2Rt = 0; var $Lower3Rt = 0;
            var $Lower1Lt = 0; var $Lower2Lt = 0; var $Lower3Lt = 0;
            var $Lower4Rt = 0; var $Lower5Rt = 0; var $Lower6Rt = 0;
            var $Lower4Lt = 0; var $Lower5Lt = 0; var $Lower6Lt = 0;
            var TotUpper6Tm = 0; var TotLower6Tm = 0; var TotUpper12 = 0;
            var TotLower12 = 0; var Anterior = 0; var Overall = 0;
            var Mandibular6Excess = 0; var Maxillary6Excess = 0;
            $Lower1Rt = parseFloat($("#Lower1Rt").val());
            $Lower2Rt = parseFloat($("#Lower2Rt").val());
            $Lower3Rt = parseFloat($("#Lower3Rt").val());
            $Lower1Lt = parseFloat($("#Lower1Lt").val());
