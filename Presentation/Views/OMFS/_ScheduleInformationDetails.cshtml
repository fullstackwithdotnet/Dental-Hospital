@model DIMS.ViewModels.OMFSIPScheduleViewModel

<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="form-group form-group-sm">
                @Html.LabelFor(x => x.FromDate, new { @class = "col-md-1" })
                <div class="col-md-1">
                    @Html.DisplayFor(x => x.FromDate, new { @class = "col-md-2" })
                </div>
                @Html.LabelFor(x => x.OPNo, new { @class = "col-md-1 " })
                <div class="col-md-2">
                    @Html.DisplayFor(x => x.OPNo, new { @class = "col-md-2 " })
                </div>
                @Html.LabelFor(x => x.PatientName, new { @class = "col-md-2 " })
                <div class="col-md-2">
                    @Html.DisplayFor(x => x.PatientName, new { @class = "col-md-2 " })
                </div>
                @Html.LabelFor(x => x.AgeGender, new { @class = "col-md-2 " })
                <div class="col-md-1">
                    @Html.DisplayFor(x => x.AgeGender, new { @class = "col-md-2 " })
                </div>
            </div>
        </div>
        
    </div>
</div>



